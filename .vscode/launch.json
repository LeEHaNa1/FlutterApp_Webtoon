    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutterwebtoon",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutterwebtoon (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutterwebtoon (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ],
    "args": [
        "chrome",
        "--web-browser-flag",
        "--disable-web-security"
    ], // web(chrome)으로 debug을 할 때, image network 문제가 생기지 않도록 하는 코드
}